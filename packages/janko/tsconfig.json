{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "outDir": "build/",
      "moduleResolution": "node"
    }
}